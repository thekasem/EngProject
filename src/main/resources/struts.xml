<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.devMode" value="false"/>
	<constant name="struts.custom.i18n.resources" value="package" />
	<package name="/" extends="struts-default">
		<action name="login" class="com.actions.UserAction" method="execute">
			<result name="success">/success.jsp</result>
			<result name="input">/index.jsp</result>
		</action>
		
		<action name="activityArchive" class="com.actions.ActivityArchiveAction" method="archive">
			<result name="add">/ActivityArchiveAdd.jsp</result>
			<result name="list">/ActivityArchiveList.jsp</result>
			<result name="search">/ActivityArchiveSearch.jsp</result>
			<result name="refresh"></result>
		</action>
		
		<action name="applicationArchive" class="com.actions.ApplicationArchiveAction" method="archive">
			<result name="add">/ApplicationArchiveAdd.jsp</result>
			<result name="list">/ApplicationArchiveList.jsp</result>
			<result name="search">/ApplicationArchiveSearch.jsp</result>
			<result name="refresh"></result>
		</action>
		
		<action name="AUMArchive" class="com.actions.AUMArchiveAction" method="archive">
			<result name="add">/AUMArchiveAdd.jsp</result>
			<result name="list">/AUMArchiveList.jsp</result>
			<result name="search">/AUMArchiveSearch.jsp</result>
			<result name="refresh"></result>
		</action>
		
	</package>

</struts>