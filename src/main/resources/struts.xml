	<package name="/" extends="struts-default">
		<action name="login_*" class="com.actions.UserAction" method="{1}">
			<result name="success">/success.jsp</result>
			<result name="fail">/index.jsp</result>
			<result name="logout">/index.jsp</result>
		</action>
		
		<action name="activityArchive_*" class="com.actions.ActivityArchiveAction" method="{1}">
			<result name="list">/pages/ActivityArchive/ActivityArchiveList.jsp</result>
			<result name="add">/pages/ActivityArchive/ActivityArchiveAdd.jsp</result>
			<result name="search">/pages/ActivityArchive/ActivityArchiveSearch.jsp</result>
			<result name="error">/success.jsp</result>
		</action>
		
		<action name="applicationArchive_*" class="com.actions.ApplicationArchiveAction" method="{1}">
